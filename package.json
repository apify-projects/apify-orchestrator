{
    "name": "apify-orchestrator",
    "version": "0.6.0",
    "description": "Utilities to orchestrate Apify Actors.",
    "engines": {
        "node": ">=16.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint ./src --ext .ts",
        "lint:fix": "eslint ./src --ext .ts --fix",
        "test": "vitest",
        "coverage": "vitest --coverage"
    },
    "keywords": [
        "apify",
        "orchestrator",
        "actor",
        "client",
        "parallel",
        "merge",
        "pipeline",
        "task",
        "concurrent",
        "optimized"
    ],
    "author": "Marco Gullo",
    "license": "Apache-2.0",
    "type": "module",
    "devDependencies": {
        "@apify/eslint-config-ts": "^0.4.1",
        "@apify/tsconfig": "^0.1.0",
        "@vitest/coverage-v8": "^3.0.5",
        "eslint": "^8.57.0",
        "typescript": "^5.7.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.5"
    },
    "dependencies": {
        "apify": "^3.2.6",
        "apify-client": "^2.12.0",
        "crawlee": "^3.11.5"
    },
    "cspell": {
        "version": "0.2",
        "ignorePaths": [],
        "dictionaryDefinitions": [
            {
                "name": "custom-dictionary",
                "path": "./.cspell/custom-dictionary.txt",
                "addWords": true
            }
        ],
        "dictionaries": [
            "custom-dictionary"
        ],
        "words": [],
        "ignoreWords": [],
        "import": []
    }
}
