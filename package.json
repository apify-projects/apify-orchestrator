{
    "name": "apify-orchestrator",
    "version": "0.6.0",
    "description": "Utilities to orchestrate Apify Actors.",
    "engines": {
        "node": ">=16.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepare": "npm run build",
        "test": "vitest run",
        "coverage": "vitest run --coverage"
    },
    "keywords": [
        "apify",
        "orchestrator",
        "actor",
        "client",
        "parallel",
        "merge",
        "pipeline",
        "task",
        "concurrent",
        "optimized"
    ],
    "author": "Marco Gullo",
    "license": "Apache-2.0",
    "type": "module",
    "devDependencies": {
        "@apify/eslint-config": "^1.1.0",
        "@apify/tsconfig": "^0.1.1",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.43.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "apify": "^3.4.5",
        "apify-client": "^2.17.0",
        "crawlee": "^3.14.1"
    },
    "cspell": {
        "version": "0.2",
        "ignorePaths": [],
        "dictionaryDefinitions": [
            {
                "name": "custom-dictionary",
                "path": "./.cspell/custom-dictionary.txt",
                "addWords": true
            }
        ],
        "dictionaries": [
            "custom-dictionary"
        ],
        "words": [],
        "ignoreWords": [],
        "import": []
    }
}
